print("원하시는 메뉴를 고르세요 : A, B, C")
menu = input()

if menu == 'A' or menu == 'a':
    while True:
        menu1 = input("정수를 입력하세요: ")
        if menu1 == '10':
            break
        else:
            continue



if menu == 'B' or menu == 'b':
    result = 0
    number = int(input("1~10사이의 수를 하나 입력하세요: "))
    for i in range(1,11):
        if not i == number:
            result += i
        else:
            continue
    print(result)

if menu == 'C' or menu == 'c':
    print("프로그램을 종료합니다.")
